#projectDetail .tab-1-title{
    padding: 15px 0;
}
#projectDetail .tab-1-name{
    text-align: center;
    line-height: 36px;
    padding: 0;
    margin: 0;
}

/*工序流程     procedure*/
#projectDetail .procedure{
	background: #fff;
	box-shadow:0px 0px 20px -8px rgba(0,0,0,.5);
	display:flex;
	border-radius:3px;
}

#projectDetail .procedure .procedure-list{
	width:350px;
	min-height:500px;
	border-right:1px solid #ccc;
	padding:15px;
	box-sizing: border-box;

}
#projectDetail .procedure .procedure-content{
	flex:1;
	padding:15px;
	box-sizing: border-box;

}
#projectDetail .procedure .procedure-content.fix{
    position: fixed;
    left: 560px;
    top: 50px;
    height: calc(100% - 100px);
    width: 100%;
    overflow-y: auto;
}
#projectDetail .procedure .procedure-list .item{
	margin-bottom:15px;
}
#projectDetail .procedure .procedure-list .item-body{
	border:1px solid #ccc;
	padding:10px;
	border-radius: 3px;
}
#projectDetail .procedure .procedure-list .num{
    color: #fff;
    border-radius: 50%;
    position: relative;
    width: 15px;
    height: 15px;
    margin: auto;
    z-index: 10;
    margin-right:10px;
    justify-content: center;
  }
#projectDetail .procedure .procedure-list .num:after{
    content: '';
    display: block;
    position: absolute;
    top: 15px;
    left: 7px;
    z-index: 5;
    width: 0;
    height: 90px;
    border-left: 1px solid rgb(184,195,206);
  }
#projectDetail .procedure .procedure-list .item:last-child .num:after{
    border: 0;
  }
#projectDetail .procedure-showdata img{
    width: 100px;
    height: 100px;
}
#projectDetail .showdata-title{
    width: 125px;
}
#projectDetail .c_m_t_10{
    margin-top: 10px;
}
#projectDetail .c_m_r_10{
    margin-right: 10px;
}
#projectDetail .c_m_b_10{
    margin-bottom: 10px;
}


/*图片显示*/
#projectDetail .part-imgshow{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 100;
}
#projectDetail .part-imgshow .part-imgshow-content,
#projectDetail .part-imgshow .part-imgshow-bg{
    position: absolute;
    top: 0;
    left: 0;
    z-index: 110;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.7);
}
#projectDetail .part-imgshow .part-imgshow-content{
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 120;
}
#projectDetail .part-imgshow img{
    max-width: 100%;
    max-height: 100%;
}
#projectDetail .tsBlock{
	width:50px;
	height:22px;
}
