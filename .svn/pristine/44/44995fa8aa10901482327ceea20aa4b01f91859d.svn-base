								<!--<div class="u-input-group-before" style="color: red;">-->

								<!--</div>-->
								<!--<input type="text" class="u-form-control" placeholder="" id="materialamount"-->
								  <!--placeholder="请填写消耗量" />-->
							<!--</div>-->
						<!--</div>-->
					<!--</div>-->
					<div class="u-row">
						<div class="u-col-md-2 tab-1-name">
							备注：
						</div>
						<div class="u-col-md-3 ">
							<div class=" u-has-feedback" >
								<!--<textarea  class="u-form-control" placeholder="请输入备注" id="memo" cols="10" rows="3"></textarea>-->
								<textarea class="u-form-control " type="text"
										  u-meta='{"type":"textarea","data":"objdatanew","field":"memo"}' placeholder="请输入备注"
								></textarea>
							</div>
						</div>
					</div>
				</div>

				<div class="u-row margin-top-15">
					<div class="padding-left-15 margin-0 ">
						<button id="add_item" class="u-msg-ok u-button u-button-primary bg_blue"
								data-bind="click: event.add_item">添加<span class="u-button-container"></span>
						</button>
						<button id="del_item" class="u-msg-ok u-button u-button-primary bg_blue"
								data-bind="click: event.del_item">删除<span class="u-button-container"></span>
						</button>
					</div>
					<div class="main-container padding-15 contacts-container">
						<div class="hr-table ">
							<div class="u-table b-table width-full" u-meta='{"id":"materialdatanew","type":"grid","data":"materialdatanew","columnWidth":"150px","editable":true}'>
								<!--<div options='{"field":"itemcode","title":"细项编码","dataType":"String","editType":"string"}'></div>-->
								<div options='{"field":"materialname","title":"材料名称","dataType":"String","editType":"string","editable":false}'></div>
								<div options='{"field":"materialamount","title":"材料消耗量","dataType":"float","editType":"float","editable":true}'></div>
							</div>
						</div>
