<!-- 列表区域  -->
<div class="u-container-fluid u-widget-bg padding-0 bg_white">
    <div class="u-widget margin-0">
        <div class="u-widget-body">
            <div class="u-row margin-left-0 margin-right-0">
                <!--树表  -->
                <div class="u-col-md-3 lefttree" id="treeT">
                    <div class="" style=" line-height: 30px;float: left;margin-left:15px;">
                        <span>组织机构</span>
                    </div>
                    <div class=" u-button-group group-spec margin-left-15" style="margin-bottom:4px;float:right;">
                    	<!--兼容ie9以下enter问题-->
                    	<!--[if lt IE 10]><button style="display:block;width:1px;height:1px;border:none;">1</button><![endif]-->
                        <button class="u-button u-button-floating " id="addorgBtn" data-bind="click: event.addorgClick">
                            <i class="uf uf-plus"></i>
                            <div class="ie8-bg">
                            	<img src="./static/plus.png" alt="..." style="width:60%;height:60%;">
                            </div>
                        </button>
                        <button class="u-button u-button-floating " id="editorgBtn" data-bind="click: event.editorgClick">
                            <i class="uf uf-pencil"></i>
                            <div class="ie8-bg">
                            	<img src="./static/pencil.png" alt="..." style="width:60%;height:60%;">
                            </div>
                        </button>
                        <button class="u-button u-button-floating del-bg" id="delorgBtn" data-bind="click: event.delorgClick">
                            <i class="uf uf-del"></i>
                            <div class="ie8-bg">
                            	<img src="./static/del.png" alt="..." style="width:60%;height:60%;">
                            </div>
                        </button>
                        <!-- org 添加、编辑组织机构-->
                        <div id="dialog_content_org" style="display:none;">
                            <div class="u-msg-title">
                                <h4>新增部门</h4>
                            </div>
                            <HR width="100%" color=#F8F9FB SIZE=3 align="center"/>
                            <div class="u-msg-content add-depart">
                                <div class="u-form">
                                    <div class="u-col-md-12 form-group">
                                        <div class="u-col-md-3 control-label">
                                            <label style="color: red;">*</label>
                                            <label>编码</label>
                                        </div>
                                        <input type="text" id="orgcode"
                                               class="u-col-md-9 u-form-control"
                                               u-meta='{"type":"string","data":"orgdatanew","field":"orgcode"}'
                                               placeholder="请输入编码">
                                    </div>
                                    <div class="u-col-md-12 form-group">
                                        <div class="u-col-md-3 control-label">
                                            <label style="color: red;">*</label>
                                            <label>组织名称</label>
                                        </div>
                                        <input type="text" id="orgname" autocomplete="off"
                                               class="u-col-md-9 u-form-control"
                                               u-meta='{"type":"string","data":"orgdatanew","field":"orgname"}'
                                               placeholder="请输入组织名称">
                                    </div>
                                    <div class="u-col-md-12 form-group">
                                        <label class="u-col-md-3 control-label">上级</label>
                                        <input type="text" id="parentname"
                                               class="u-col-md-9 u-form-control"
                                               u-meta='{"type":"string","data":"orgdatanew","field":"parentname"}'
                                               placeholder="上级"
                                               disabled="disabled">
                                    </div>
                                    <div class="u-col-md-12 form-group" style="display:none">
                                        <label class="u-col-md-3 control-label">父ID</label>
                                        <input type="text" id="parentid"
                                               class="u-col-md-9 u-form-control"
                                               u-meta='{"type":"string","data":"orgdatanew","field":"parentid"}'
                                               placeholder="请输入父ID">
                                    </div>
                                    <div class="u-col-md-12 form-group">
                                        <label class="u-col-md-3 control-label">组织名称缩写</label>
                                        <input type="text" id="shortname"
                                               class="u-col-md-9 u-form-control"
                                               u-meta='{"type":"string","data":"orgdatanew","field":"shortname"}'
                                               placeholder="请输入缩写">
                                    </div>
                                </div>
                            </div>
                            <div class="u-msg-footer">
                                <div class="pull-right">
                                    <button id="canceltree" class="u-button editCancel raised back"
                                            data-bind="click: event.cancelorgClick">取消<span
                                            class="u-button-container"><span
                                            class="u-ripple"></span></span>
                                    </button>

                                    <button id="savetree" class="u-button raised u-button-primary margin-right-10 bg_blue"
                                            data-bind="click: event.saveorgClick">
                                                                                         保存<span class="u-button-container"><span class="u-ripple"></span></span>
                                    </button>
                                </div>
                            </div>
                        </div>
                        <HR class="hide" width="100%" color=#F8F9FB SIZE=30 align="center"/>
                    </div>
                    <!--tree 组织机构 -->
                    <div class="u-col-md-3" id="tree2Around" style="width:100%;">
                        <div id="tree2" class="ztree"
                             u-meta='{"id":"tree2","data":"orgdata","type":"tree","idField":"orgid","nameField":"orgname","pidField":"parentid","shortname":"shortname","setting":"treeSetting"}'>
                        </div>
                    </div>
                </div>

                <!-- user 用户列表   -->
                <div class="u-col-md-9 padding-left-0 padding-right-0" style="border-left:1px solid #cecece;">
                    <div class="infoPanel" id="infoPanel"></div>
                    <div class="treefrom">
                        <div class="ec-search u-col-md-12 u-col-sm-12  padding-15">
                            <div class="pull-right">
                                <!--<div class="margin-right-10 pull-right">
                                    <input type="button" id="searchBtn" class="margin-right-10 u-button u-buttom-sm"
                                           value="搜索" data-bind="click: event.searchClick">
                                </div>
                                <div id="searchdiv" class="margin-right-0 pull-right u-has-feedback input_enter">
                                    <input type="text" id="searchInp" class="u-form-control c-sm"
                                           placeholder="姓名/手机号/邮箱">
                                    <span class="u-form-control-feedback-1  uf-search-light-0 " id="returnBtn"
                                          data-bind="click: event.returnClick" style="right:0;top:-4px;">×</span>
                                </div>-->
                                <div id="searchdiv" class="margin-right-10 pull-right u-has-feedback input_enter">
                                    <input type="text" class="u-form-control c-sm" id="searchInp" placeholder="姓名/手机号/邮箱">
                                    <span class="uf zoom uf-search-light-2" id="searchBtn" data-bind="click: event.searchClick"></span>
                                    <span class="u-form-control-feedback-1  uf-search-light-0 uf uf-close" id="returnBtn"
                                          data-bind="click: event.returnClick"></span>
                                </div>
                                <div class="pull-right" style="border-left:none;">
                                    <input type="button" class="margin-right-15 u-button u-button-white u-buttom-sm blacklist"
                                           value="黑名单" data-bind="click: event.blList">
                                </div>
                                <div class="margin-right-0 pull-right">
                                    <input type="button" class="margin-right-0 u-button u-button-white u-buttom-sm whitelist"
                                           value="白名单" data-bind="click: event.wlList">
                                </div>
                                <div class="pull-right">
                                    <input type="button" class="u-button u-button-primary u-buttom-sm bg_blue margin-right-15"
                                           id="add_man" value="新增人员"
                                           style="" data-bind="click: event.addManClick">
                                </div>
                            </div>
                            <!--user  添加。 编辑 用户 -->
                            <div id="dialog_content_man" class="modal-dialog" style="display:none;">
                                <div class="u-msg-title">
                                    <h4>新增人员</h4>
                                </div>
                                <HR width="100%" color=#F8F9FB SIZE=3 align="center"/>
                                <div class="u-msg-content add-man">
                                    <div class="u-form" id="add-form">
                                        <div class="u-col-md-12 form-group c_align_center">
                                            <div class="avatarUrl">
                                                <img width='100px' height='100px' style='border-radius:50%;' src="static/constructionphoto.jpg">
                                            </div>
                                        </div>
                                        <div class="u-col-md-12 form-group">
                                            <div class="u-col-md-3 control-label">
                                                <label style="color: red;">*</label>
                                                <label>姓名</label>
                                            </div>
                                            <input class="u-col-md-9 u-form-control " type="text" id="add_username"
                                                   u-meta='{"type":"string","data":"userdatanew","field":"username"}'
                                                   placeholder="请输入姓名"
                                                    />
                                           </div>

                                        <div class="u-col-md-12 form-group">
                                            <div class="u-col-md-3 control-label">
                                            	<label style="color: red;">*</label>
                                                <label >手机号码</label>
                                            </div>
                                            <input class="u-col-md-9 u-form-control " type="text" id="add_usermobile"
                                                   u-meta='{"type":"string","data":"userdatanew","field":"usermobile"}'
                                                   placeholder="请输入办公电话"
                                                    />
                                        </div>
                                        <div class="u-col-md-12 form-group">
                                            <div class="u-col-md-3 control-label">
                                            	<label style="color: red;">*</label>
                                                <label >密码</label>
                                            </div>
                                            <input class="u-col-md-9 u-form-control " type="text" id="add_userpassword"
                                                   u-meta='{"type":"string","data":"userdatanew","field":"userpassword"}'
                                                   placeholder="请输入密码"
                                                    />
                                        </div>
                                        <div class="u-col-md-12 form-group" style="">
                                            <div class="u-col-md-3 control-label">
                                                <label style="color: red;">*</label>
                                                <label>角色</label>
                                            </div>
                                            <div class="u-col-md-9 u-form-control " id="comboSex" style="padding:0;"
                                         u-meta='{"type":"u-combobox","datasource":"roledata","data":"userdatanew","field":"levelid","onlySelect":true}'>
		                                        <div class="">
		                                            <!-- <div class="u-input-group-before" style="color: red;">*</div> -->
		                                            <span class="u-form-control-feedback fa fa-angle-down"
		                                                  data-role="combo-button" ></span>
		                                            <input placeholder="请选择角色" class="u-form-control" style="border:0;width:100%;height:100%;background:#fff;" readonly/>
		                                        </div>
		                                    </div>
                                        </div>
                                        <div class="u-col-md-12 form-group" style="">
                                            <div class="u-col-md-3 control-label">
                                                <!-- <label style="color: red;">*</label> -->
                                                <label>员工编号</label>
                                            </div>
                                            <input class="u-col-md-9 u-form-control " type="text" id="add_usercode"
                                                   u-meta='{"type":"string","data":"userdatanew","field":"usercode"}'
                                                   placeholder="请输入员工编号"
                                                    />
                                        </div>
                                        <div class="u-col-md-12 form-group" style="">
                                            <div class="u-col-md-3 control-label">
                                                <!-- <label style="color: red;">*</label> -->
                                                <label>电子邮箱</label>
                                            </div>
                                            <input class="u-col-md-9 u-form-control " type="text" id="add_email"
                                                   u-meta='{"type":"string","data":"userdatanew","field":"useremail"}'
                                                   placeholder="请输入电子邮箱"
                                                    />
                                        </div>

                                        <div class="u-col-md-12 form-group">
                                            <div class="u-col-md-3 control-label">
                                                <label >所属机构</label>
                                            </div>
                                            <input class="u-col-md-9 u-form-control " type="text" id="add_orgname"
                                                   placeholder="请输入所属机构" disabled="disabled"
                                                   u-meta='{"type":"string","data":"userdatanew","field":"orgname"}'
                                                    />
                                        </div>
                                        <div class="u-col-md-12 form-group" style="display:none;">
                                            <div class="u-col-md-3 control-label">
                                                <label >所属机构</label>
                                            </div>
                                            <input class="u-col-md-9 u-form-control " type="text" id="add_orgid"
                                                   placeholder="请输入所属机构" disabled="disabled"
                                                   u-meta='{"type":"string","data":"userdatanew","field":"orgid"}'
                                                    />
                                        </div>
                                    </div>
                                </div>
                                <div class="u-msg-footer">
                                    <button id="saveman" class="u-msg-ok u-button u-button-primary bg_blue"
                                            data-bind="click: event.saveManClick">保存
                                        <span class="u-button-container"></span>
                                    </button>
                                    <button id="cancelman" class="u-msg-cancel u-button u-button-white"
                                            data-bind="click: event.cancelManClick">取消
                                        <span class="u-button-container"></span>
                                    </button>

                                </div>
                            </div>

                            <!--弹出确认框-->
                            <div class="u-col-md-12">
                                <div id="dialog_bl" style="display:none;">
                                    <div class="u-msg-title">
                                        <h4>加入/移除黑名单</h4>
                                    </div>
                                    <HR width="100%" color=#F8F9FB SIZE=3 align="center"/>
                                    <div class="u-msg-content">
                                        <div class="u-row margin-bottom-20 left" style="padding-left:72px;">
                                            <p class="font-size-16 remind" style=""> </p>
                                        </div>
                                        <div class="u-row margin-bottom-20">
                                            <div class="u-col-3 ">
                                                <label for="code" class="u-input-label right font-size-16">原因:</label>
                                            </div>
                                            <div class="u-col-7">
                                                <div class="u-input-group-before padding-left-10 "
                                                     style="color: red;">*
                                                </div>
                                                <input type="text" id="blInp" class="u-form-control padding-left-20" placeholder="请填写原因">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="u-msg-footer">
                                        <button class="u-msg-ok u-button primary" data-bind="click: event.saveClick">确认<span class="u-button-container"><span class="u-ripple"></span></span>
                                        </button>
                                        <button class="u-msg-cancel u-button  u-grey" data-bind="click: event.cancelClick">取消<span class="u-button-container"><span class="u-ripple"></span></span>
                                        </button>
                                    </div>
                                </div>
                            </div>

                            <!--黑名单操作记录-->
                            <div id="dialog_record" tabindex="-1" role="dialog" style="display:none;">
                                <div class="u-msg-title">
                                    <h4 class="treetitle">黑名单操作记录</h4>
                                </div>
                                <div class="u-msg-content">
                                    <div class="hr-table">
                                        <div class="u-table b-table width-full"
                                             u-meta='{"id":"blrecord","type":"grid","data":"blacklist_data","columnWidth":"150px","editType":"default","columnMenu":true}'>
                                            <div options='{"field":"username","title":"工人姓名","editable":false,"dataType":"string"}'></div>
                                            <div options='{"field":"createname","title":"操作人","editable":false,"dataType":"string"}'></div>
                                            <div options='{"field":"memotype","title":"操作内容","editable":false,"dataType":"string"}'></div>
                                            <div options='{"field":"memo","title":"原因","editable":false,"dataType":"string"}'></div>
                                            <div options='{"field":"ts","title":"操作时间","editable":false,"dataType":"data","renderType":"event.renderDatetime"}'></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="u-msg-footer">
                                    <button type="button" id="confirm_select_material" class="u-button u-button-primary pull-right" data-bind="click: event.cancelClick">
                                        确定
                                    </button>
                                </div>
                            </div>
                        </div>



                        <!--user 展示 用户列表 -->
                        <div class="main-container padding-0 contacts-container">
                            <div class="hr-table ">
                                <div class="u-table b-table width-full"
                                     u-meta='{"id":"userdata","type":"grid","data":"userdata","columnWidth":"150px"}'>
                                    <div
                                            options='{"field":"username","title":"姓名","dataType":"String","editType":"string","renderType":"event.renderTypeTitle"}'></div>
                                    <div
                                            options='{"field":"usercode","title":"员工编号","dataType":"String","editType":"string"}'></div>
                                    <div
                                            options='{"field":"orgname","title":"所属机构","dataType":"String","editType":"string"}'></div>
                                    <div
                                            options='{"field":"usermobile","title":"手机号码","dataType":"String","editType":"string"}'>
                                    </div>
                                    <div
                                            options='{"field":"levelname","title":"角色","dataType":"String","editType":"string"}'>
                                    </div>
                                    <!--<div style="display:none;"-->
                                            <!--options='{"field":"levelid","title":"角色id","dataType":"String","editType":"string"}'>-->
                                    <!--</div>-->
                                    <div
                                            options='{"field":"useremail","title":"电子邮箱","dataType":"String","editType":"string"}'>
                                    </div>
                                    <div options='{"field":"operate","dataType":"String","title":"操作","renderType":"event.optFun","editable":true}'></div>
                                </div>
                            </div>
                            <div id='pagination' class='pagination u-pagination pagination-sm'></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>




