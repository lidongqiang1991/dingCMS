html, body, div, span, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, address, big, cite, code, del, em, font, img, ins, small, strong, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend {
    margin: 0;
    padding: 0;
}
input:-webkit-autofill {
    -webkit-box-shadow: 0 0 0px 1000px white inset;
}
body {
    margin: 0 auto;
}
body {
    color: #333;
    font-family: Arial,"宋体",Lucida,Verdana,Helvetica,sans-serif;
    font-size: 12px;
    line-height: 150%;
}
body {
    background: none repeat scroll 0 0 #fff;
}

#entry {
    background: none repeat scroll 0 0 #fff;
    border-radius: 3px;
    margin: 60px auto 55px;
    padding: 5px;
    /*position: relative;*/
}
.m, .mt, .mc, .mb {
    overflow: hidden;
}
#entry .mc {
    background: none repeat scroll 0 0 #ffffff;
    /*border: 1px solid #e1e1e1;*/
    width: 565px;
    /*height: 355px;*/
    margin: auto;
    text-align: center;
}
#entry .form {
    /*height: 354px;*/
    /*overflow: hidden;*/
    /*position: relative;*/
    /*right: 32px;*/
    /*top: 18px;*/
    width: 400px;
    margin: auto;
}

#formlogin .item-ifo input{
    width: 400px;
    height: 47px;
    margin-top: 34px;
    padding: 15px 24px;
    border: 1px solid #ccc;
    border-radius: 8px;
}

#formlogin .opera{
    margin-top: 28px;
    font-size: 15px;
}
#formlogin .opera::before,
#formlogin .opera::after{
    content: '';
    display: block;
    clear: both;
}
#formlogin .bg_blue{
    background:rgb(3,162,228);
}
#formlogin .login-btn2013{
    margin-top: 34px;

}
#formlogin .login-btn2013 button{
    width: 400px;
    height: 45px;
    line-height: 45px;
    background:#03a2e4 ;
    border: 1px solid #03a2e4;
    color: #fff;
    font-size: 20px;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 8px;
    cursor: pointer;
}
#formlogin input,button{outline:none !important;}

