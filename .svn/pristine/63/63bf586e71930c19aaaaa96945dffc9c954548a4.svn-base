		<!--列表展示-->
		<div class="main-container padding-top-0 contacts-container">
			<div class="hr-table ">
				<div class="u-table b-table width-full"
					 u-meta='{"id":"productitemdata","type":"grid","data":"objdata","columnWidth":"150px","onDblClickFun":"event.selectClick"}'>
					<div options='{"field":"name","title":"规则名称","dataType":"String","editType":"string","renderType":"event.renderTypeAjax"}'></div>
					<div options='{"field":"performancetype","title":"业绩类型","dataType":"String","editType":"string","renderType":"event.renderPerformancetype"}'></div>
					<div options='{"field":"rewardratio","title":"奖励比例","dataType":"String","editType":"string","renderType":"event.renderRewardratio"}'></div>
					<div options='{"field":"creator","title":"创建人","dataType":"String","editType":"string"}'></div>
					<div options='{"field":"creationtime","title":"创建时间","dataType":"String","editType":"string","renderType":"event.renderDatetime"}'></div>
					<div options='{"field":"enable","title":"是否启用","dataType":"String","editType":"string","renderType":"event.renderEnable"}'></div>
				</div>
			</div>
			<div id='pagination' class='pagination u-pagination pagination-sm'></div>
		</div>

	</div>
</div>
